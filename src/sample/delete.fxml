<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="650.0" minWidth="1290.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #C4FCF0;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Button fx:id="Delete_existing" layoutX="500.0" layoutY="279.0" mnemonicParsing="false" onAction="#deleteItem" prefHeight="40.0" prefWidth="320.0" style="-fx-background-color: #005575;" text="DELETE EXISTING ITEM" textFill="#faf2f2">
            <font>
                <Font name="Arial Black" size="18.0" />
            </font>
        </Button>
        <Button fx:id="Delete_specific" layoutX="500.0" layoutY="350.0" mnemonicParsing="false" onAction="#deleteHistory" prefHeight="40.0" prefWidth="320.0" style="-fx-background-color: #005575;" text="DELETE SPECIFIC PRODUCT" textFill="#f7f6f6">
            <font>
                <Font name="Arial Black" size="18.0" />
            </font>
        </Button>
      <Button fx:id="back" layoutX="-6.0" mnemonicParsing="false" onAction="#goThird" prefHeight="46.0" prefWidth="121.0" style="-fx-background-color: #C4FCF0;" text="&lt;- BACK" textAlignment="JUSTIFY" textFill="#005575">
