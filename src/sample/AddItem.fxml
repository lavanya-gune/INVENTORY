<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="650.0" minWidth="1290.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #C4FCF0;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Button fx:id="new_item" layoutX="480.0" layoutY="310.0" mnemonicParsing="false" onAction="#AddNewItem" prefHeight="40.0" prefWidth="344.0" style="-fx-background-color: #005575;" text="ADD NEW ITEM" textFill="#f7f4f4">
            <font>
                <Font name="Arial Black" size="18.0" />
            </font>
        </Button>
        <Button fx:id="existing_item" layoutX="480.0" layoutY="250.0" mnemonicParsing="false" onAction="#AddExistingItem" prefHeight="40.0" prefWidth="344.0" style="-fx-background-color: #005575;" text="ADD TO THE EXISTING ITEM" textFill="#f2f1e5">
            <font>
                <Font name="Arial Black" size="18.0" />
            </font>
        </Button>
        <Button fx:id="back" mnemonicParsing="false" onAction="#goThird" prefHeight="46.0" prefWidth="121.0" style="-fx-background-color: #C4FCF0;" text="&lt;- BACK" textAlignment="JUSTIFY" textFill="#005575">
            <font>
                <Font name="System Bold" size="21.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
